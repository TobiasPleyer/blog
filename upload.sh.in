HOST='{{ ftp_host }}'
USER='{{ user }}'

echo -n "Password for $USER: "
read -s PASSWD

ftp -n $HOST <<END_SCRIPT
quote USER $USER
quote PASS $PASSWD
{% for lfile,rfile in sync_map %}
put {{lfile}} {{rfile}}
{% endfor %}
quit
END_SCRIPT
exit 0
